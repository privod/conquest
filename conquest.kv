#:kivy 2.0.0

<Location>
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size

<Land>
    canvas:
        Color:
            rgba: 0, 1, 0, 0.8

<Ocean>
    canvas:
        Color:
            rgba: 0, 0, 1, 1.8

<Map>
    padding: 5
    spacing: 5

<ConquestGame>
    map: map_id

    Map:
        id: map_id